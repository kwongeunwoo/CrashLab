<?xml version="1.0" ?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find crash_simulation)/description/materials.xacro" />
<!-- <xacro:include filename="$(find crash_simulation)/description/crash_robot.trans" /> -->
<!-- <xacro:include filename="$(find crash_simulation)/description/crash_robot.gazebo" /> -->
<link name="base_link">
  <inertial>
    <origin xyz="-0.10830441512900894 0.0006958464779866047 0.10833623035608012" rpy="0 0 0"/>
    <!-- <mass value="75.90258337879241"/> -->
    <mass value="30.0"/>
    <inertia ixx="0.899846" iyy="0.838355" izz="0.979941" ixy="-0.009374" iyz="-0.009083" ixz="-0.031782"/>
  </inertial>
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find crash_simulation)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find crash_simulation)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<joint name="base_footprint_joint" type="fixed">
  <parent link="base_link"/>
  <child link="base_footprint"/>
  <origin xyz="0 0 0" rpy="0 0 0"/>
</joint>

<link name="base_footprint">
</link>

