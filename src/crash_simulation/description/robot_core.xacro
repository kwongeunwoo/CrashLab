<link name="caster_1">
  <inertial>
    <origin xyz="-0.01751980598239028 4.3013562483747607e-08 -0.05240104554531534" rpy="0 0 0"/>
    <mass value="1.6117880362991714"/>
    <inertia ixx="0.001424" iyy="0.001841" izz="0.000923" ixy="-0.0" iyz="0.0" ixz="-0.000298"/>
  </inertial>
  <visual>
    <origin xyz="0.279 0.0 -0.043" rpy="0 0 0"/>
